{
  "name": "ag-virtual-scroll",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "deploy-git-pages": "ng build demo -c=production --base-href=/ag-virtual-scroll/ && node node_modules/gh-pages/bin/gh-pages -d ./dist/demo",
    "build-and-publish": "npm run build-lib && npm run publish-pkg",
    "build-lib": "ng build library && node copy && cd dist/library && npm pack",
    "publish-pkg": "pushd dist/library && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.1.4",
    "@angular/cdk": "20.1.4",
    "@angular/common": "20.1.4",
    "@angular/compiler": "20.1.4",
    "@angular/core": "20.1.4",
    "@angular/forms": "20.1.4",
    "@angular/material": "20.1.4",
    "@angular/platform-browser": "20.1.4",
    "@angular/platform-browser-dynamic": "20.1.4",
    "@angular/router": "20.1.4",
    "@faker-js/faker": "^8.3.1",
    "ngx-highlightjs": "^10.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.4.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "20.1.4",
    "@types/jasmine": "~4.3.0",
    "cpx": "^1.5.0",
    "gh-pages": "^6.2.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.1.0",
    "typescript": "5.8.3"
  }
}
