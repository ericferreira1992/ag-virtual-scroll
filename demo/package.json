{
  "name": "ag-virtual-scroll",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "pushd demo && ng serve",
    "build": "pushd demo && ng build -c=production --aot",
    "deploy-git-pages": "pushd demo && ng build -c=production --aot --base-href=/ag-virtual-scroll/ && node ../node_modules/gh-pages/bin/gh-pages -d dist",
    "build-lib": "rimraf dist && npm run packagr && node copy && cd dist && npm pack",
    "publish-pkg": "pushd dist && npm publish",
    "build-and-publish": "npm run build-lib && npm run publish-pkg",
    "packagr": "ng-packagr -p src/ng-package.json"
  },
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.5",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/core": "^17.0.5",
    "@angular/forms": "^17.0.5",
    "@angular/material": "^16.2.12",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "@faker-js/faker": "^7.5.0",
    "core-js": "^2.6.11",
    "express": "^4.17.1",
    "fast-deep-equal": "^2.0.1",
    "gh-pages": "^2.2.0",
    "highlight.js": "^11.6.0",
    "ngx-highlightjs": "^7.0.1",
    "path": "^0.12.7",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.5",
    "@angular/cli": "^17.0.5",
    "@angular/compiler-cli": "^17.0.5",
    "@angular/language-service": "^17.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "cpx": "^1.5.0",
    "ng-packagr": "^17.0.2",
    "protractor": "~7.0.0",
    "ts-node": "~5.0.1",
    "tsickle": "^0.46.0",
    "tslint": "~6.1.0",
    "typescript": "^5.2.2"
  }
}